pipeline {
    agent { label 'slave' }
    
  stages {
    stage('Test') {
      steps {
        sh 'docker rm -v $(docker ps -aq -f status=exited)'
        //sh 'docker rm $(docker ps -a -f status=created -q)'
        //sh  'docker rm $(docker ps -a -q)'
        sh 'docker run -d --name apache -p 80:80 httpd'
      }
    }
  }
}
